{
    "name": "jardin-de-vicky/nova-file-manager",
    "description": "File Manager tool for Laravel Nova 4",
    "keywords": [
        "laravel",
        "nova",
        "file-manager"
    ],
    "homepage": "https://github.com/jardin-de-vicky/nova-filemanager",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Lagarda",
            "email": "eric@infinety.es",
            "role": "Legacy Developer"
        },
        {
            "name": "InfinetyEs",
            "email": "apps@infinety.es",
            "role": "Legacy Maintainer"
        },
        {
            "name": "Mathias Beaulieu-Duncan",
            "email": "mathias@openharbor.io",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "laravel/nova": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "9.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JardinDeVicky\\NovaFileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JardinDeVicky\\NovaFileManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JardinDeVicky\\NovaFileManager\\FileManagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}